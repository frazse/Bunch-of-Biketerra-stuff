{
  "manifest_version": 3,
  "name": "Power Overlay",
  "version": "1.0",
  "description": "Visar Power/HR/Cadence i overlay.",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
